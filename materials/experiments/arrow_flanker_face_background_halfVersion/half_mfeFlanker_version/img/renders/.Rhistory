library("tidyverse")
library("psych")
library("car")
library("dplyr")
library("effsize")
# To create a matrix of our data and then, convert it to a table
data <- matrix(c(98,88,95,75,100,88,75,66,95,83,73,75,60,70,72), nrow=5, ncol=3, byrow= FALSE)
colnames(data) <- c('In-Person', 'At home', 'In Classroom')
data <- as.table(data)
data <- as.data.frame(data)
data = select(data, -1)
data <- as_tibble(data)
data <- rename(data, values=Freq)
data <- rename(data, groups=Var2)
view(data)
data$groups <- as_factor(data$groups)
view(data)
data %>%
group_by(groups) %>%
summarise(mean=mean(values), sd=sd(values))
